{
  "name": "@pharen/ui",
  "version": "0.0.12",
  "description": "Pharen UI â€“ Modern Vue 3 component library inspired by Shadcn, built for Tailwind v4.",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.es.js"
    },
    "./styles.css": "./dist/ui.css"
  },
  "bin": {
    "pharen-ui": "./dist/cli.js"
  },
  "files": [
    "dist",
    "scripts/cli.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm run build:cli",
    "build:cli": "esbuild scripts/cli.ts --bundle --platform=node --outfile=dist/cli.js --banner:js='#!/usr/bin/env node'",
    "lint": "eslint src --ext .ts,.vue"
  },
  "keywords": [
    "vue",
    "ui-library",
    "tailwind",
    "shadcn",
    "components",
    "pharen",
    "cli"
  ],
  "author": "Pharen",
  "license": "MIT",
  "peerDependencies": {
    "tailwindcss": "^4.1.5",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.6.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/integrations": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "^8.6.0",
    "esbuild": "^0.25.10",
    "kleur": "^4.1.5",
    "lodash-es": "^4.17.21",
    "reka-ui": "^2.5.1",
    "sortablejs": "^1.15.6",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "3.1.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.22",
    "vue-sonner": "^2.0.9",
    "zod": "^4.1.11"
  },
  "publishConfig": {
    "access": "public"
  }
